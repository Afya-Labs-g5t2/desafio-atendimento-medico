{
  "name": "server",
  "version": "0.0.1",
  "description": "api CRUD",
  "main": "server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "populate": "npx sequelize-cli db:migrate && npx sequelize-cli db:seed:all",
    "remove": "npx sequelize-cli db:migrate:undo:all",
    "test": "NODE_ENV=test jest",
    "pretest": "NODE_ENV=test sequelize db:migrate",
    "posttest": "NODE_ENV=test sequelize db:migrate:undo:all"
  },
  "keywords": [
    "server",
    "api",
    "swagger",
    "express",
    "sequelize",
    "jwt",
    "bcrypt",
    "postgres"
  ],
  "author": "g5t2team",
  "license": "MIT",
  "dependencies": {
    "bcrypt": "^5.0.1",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "pg": "^8.6.0",
    "pg-hstore": "^2.3.3",
    "sequelize": "^6.6.2",
    "swagger-jsdoc": "^6.1.0",
    "swagger-ui-express": "^4.1.6"
  },
  "devDependencies": {
    "nodemon": "^2.0.7",
    "sequelize-cli": "^6.2.0",
    "jest": "^27.0.4",
    "sqlite3": "^5.0.2",
    "supertest": "^6.1.3"
  }
}
